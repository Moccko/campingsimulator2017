<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="fr.iut.model.AuthorizationEntity" table="Authorization" schema="CampingSimulator">
        <id name="idAuthorization">
            <column name="idAuthorization" sql-type="int(11)"/>
        </id>
        <property name="label">
            <column name="label" sql-type="varchar(45)" length="45" not-null="true"/>
        </property>
        <set name="userHasAuthorizationsByIdAuthorization" inverse="true">
            <key>
                <column name="idAuthorization"/>
            </key>
            <one-to-many not-found="ignore" class="fr.iut.model.UserHasAuthorizationEntity"/>
        </set>
    </class>
</hibernate-mapping>