<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="fr.iut.model.RestockingEntity" table="Restocking" schema="CampingSimulator">
        <id name="idRestocking">
            <column name="idRestocking" sql-type="int(11)"/>
        </id>
        <property name="quantity">
            <column name="quantity" sql-type="int(11)"/>
        </property>
        <property name="datetime">
            <column name="datetime" sql-type="datetime"/>
        </property>
        <property name="idSupplier">
            <column name="idSupplier" sql-type="int(11)"/>
        </property>
        <property name="idProduct">
            <column name="idProduct" sql-type="int(11)"/>
        </property>
        <many-to-one name="supplierByIdSupplier" class="fr.iut.model.SupplierEntity">
            <column name="idSupplier"/>
        </many-to-one>
        <many-to-one name="productByIdProduct" class="fr.iut.model.ProductEntity">
            <column name="idProduct"/>
        </many-to-one>
    </class>
</hibernate-mapping>