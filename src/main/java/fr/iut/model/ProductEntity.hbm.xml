<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="fr.iut.model.ProductEntity" table="Product" schema="CampingSimulator">
        <id name="idProduct">
            <column name="idProduct" sql-type="int(11)"/>
        </id>
        <property name="stock">
            <column name="stock" sql-type="int(11)"/>
        </property>
        <property name="sellPrice">
            <column name="sell_price" sql-type="float" precision="-1"/>
        </property>
        <property name="label">
            <column name="label" sql-type="varchar(45)" length="45"/>
        </property>
        <set name="purchasesByIdProduct" inverse="true">
            <key>
                <column name="idProduct"/>
            </key>
            <one-to-many not-found="ignore" class="fr.iut.model.PurchaseEntity"/>
        </set>
        <set name="restockingsByIdProduct" inverse="true">
            <key>
                <column name="idProduct"/>
            </key>
            <one-to-many not-found="ignore" class="fr.iut.model.RestockingEntity"/>
        </set>
        <set name="supplierHasProductsByIdProduct" inverse="true">
            <key>
                <column name="idProduct"/>
            </key>
            <one-to-many not-found="ignore" class="fr.iut.model.SupplierHasProductEntity"/>
        </set>
    </class>
</hibernate-mapping>