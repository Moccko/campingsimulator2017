<?xml version="1.0" encoding="UTF-8"?>
<entity-mappings xmlns="http://java.sun.com/xml/ns/persistence/orm"
        version="2.0">

    <entity class="fr.iut.model.Problem">
        <table name="Problem" schema="CampingSimulator" catalog=""/>
        <attributes>
            <id name="id">
                <column name="id" precision="11"/>
            </id>
            <basic name="appearanceDatetime">
                <column name="appearance_datetime" precision="-1"/>
            </basic>
            <basic name="label">
                <column name="label" length="45"/>
            </basic>
            <basic name="solutionDatetime">
                <column name="solution_datetime" nullable="false" precision="-1"/>
            </basic>
            <basic name="state">
                <column name="state" length="45"/>
            </basic>
        </attributes>
    </entity>
</entity-mappings>
