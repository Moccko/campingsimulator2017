<?xml version="1.0" encoding="UTF-8"?>
<entity-mappings xmlns="http://java.sun.com/xml/ns/persistence/orm"
        version="2.0">

    <entity class="fr.iut.model.Notification">
        <table name="Notification" schema="CampingSimulator" catalog=""/>
        <attributes>
            <id name="id">
                <column name="id" precision="11"/>
            </id>
            <basic name="title">
                <column name="title" length="45"/>
            </basic>
            <basic name="content">
                <column name="content" length="200"/>
            </basic>
        </attributes>
    </entity>
</entity-mappings>
