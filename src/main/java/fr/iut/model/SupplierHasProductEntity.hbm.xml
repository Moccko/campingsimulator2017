<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="fr.iut.model.SupplierHasProductEntity" table="Supplier_has_Product" schema="CampingSimulator">
        <composite-id mapped="true" class="fr.iut.model.SupplierHasProductEntityPK">
            <key-property name="idSupplier">
                <column name="idSupplier" sql-type="int(11)"/>
            </key-property>
            <key-property name="idProduct">
                <column name="idProduct" sql-type="int(11)"/>
            </key-property>
        </composite-id>
        <property name="buyPrice">
            <column name="buy_price" sql-type="float" precision="-1"/>
        </property>
        <many-to-one name="supplierByIdSupplier" class="fr.iut.model.SupplierEntity">
            <column name="idSupplier"/>
        </many-to-one>
        <many-to-one name="productByIdProduct" class="fr.iut.model.ProductEntity">
            <column name="idProduct"/>
        </many-to-one>
    </class>
</hibernate-mapping>