<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="fr.iut.model.LogEntity" table="Log" schema="CampingSimulator">
        <id name="idLog">
            <column name="idLog" sql-type="int(11)"/>
        </id>
        <property name="datetime">
            <column name="datetime" sql-type="datetime"/>
        </property>
        <property name="action">
            <column name="action" sql-type="varchar(150)" length="150"/>
        </property>
        <property name="user">
            <column name="user" sql-type="varchar(20)" length="20"/>
        </property>
        <many-to-one name="userByUser" class="fr.iut.model.UserEntity">
            <column name="user"/>
        </many-to-one>
    </class>
</hibernate-mapping>