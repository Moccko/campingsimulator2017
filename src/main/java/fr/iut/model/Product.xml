<?xml version="1.0" encoding="UTF-8"?>
<entity-mappings xmlns="http://java.sun.com/xml/ns/persistence/orm"
        version="2.0">

    <entity class="fr.iut.model.Product">
        <table name="Product" schema="CampingSimulator" catalog=""/>
        <attributes>
            <id name="id">
                <column name="id" precision="11"/>
            </id>
            <basic name="stock">
                <column name="stock" precision="11"/>
            </basic>
            <basic name="sellPrice">
                <column name="sell_price" precision="-1"/>
            </basic>
            <basic name="label">
                <column name="label" length="45"/>
            </basic>
        </attributes>
    </entity>
</entity-mappings>
