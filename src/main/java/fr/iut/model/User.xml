<?xml version="1.0" encoding="UTF-8"?>
<entity-mappings xmlns="http://java.sun.com/xml/ns/persistence/orm"
        version="2.0">

    <entity class="fr.iut.model.User">
        <table name="User" schema="CampingSimulator" catalog=""/>
        <attributes>
            <id name="id">
                <column name="id" precision="11"/>
            </id>
            <basic name="login">
                <column name="login" length="20"/>
            </basic>
            <basic name="password">
                <column name="password"/>
            </basic>
            <basic name="employeeId">
                <column name="Employee_id" nullable="false" precision="11"/>
            </basic>
        </attributes>
    </entity>
</entity-mappings>
