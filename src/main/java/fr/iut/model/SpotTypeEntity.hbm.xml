<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="fr.iut.model.SpotTypeEntity" table="SpotType" schema="CampingSimulator">
        <id name="idSpotType">
            <column name="idSpotType" sql-type="int(11)"/>
        </id>
        <property name="label">
            <column name="label" sql-type="varchar(45)" length="45"/>
        </property>
        <set name="spotsByIdSpotType" inverse="true">
            <key>
                <column name="table1_SpotType"/>
            </key>
            <one-to-many not-found="ignore" class="fr.iut.model.SpotEntity"/>
        </set>
    </class>
</hibernate-mapping>