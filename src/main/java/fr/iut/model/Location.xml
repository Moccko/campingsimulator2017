<?xml version="1.0" encoding="UTF-8"?>
<entity-mappings xmlns="http://java.sun.com/xml/ns/persistence/orm"
        version="2.0">

    <entity class="fr.iut.model.Location">
        <table name="Location" schema="CampingSimulator" catalog=""/>
        <attributes>
            <id name="id">
                <column name="id" precision="11"/>
            </id>
            <basic name="name">
                <column name="name" length="45"/>
            </basic>
            <basic name="pointX">
                <column name="point_x" nullable="false" precision="-1"/>
            </basic>
            <basic name="pointY">
                <column name="point_y" nullable="false" precision="-1"/>
            </basic>
        </attributes>
    </entity>
</entity-mappings>
