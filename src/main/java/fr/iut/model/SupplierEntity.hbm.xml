<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="fr.iut.model.SupplierEntity" table="Supplier" schema="CampingSimulator">
        <id name="idSupplier">
            <column name="idSupplier" sql-type="int(11)"/>
        </id>
        <property name="name">
            <column name="name" sql-type="varchar(45)" length="45"/>
        </property>
        <property name="phone">
            <column name="phone" sql-type="varchar(11)" length="11" not-null="true"/>
        </property>
        <property name="email">
            <column name="email" sql-type="varchar(45)" length="45" not-null="true"/>
        </property>
        <property name="website">
            <column name="website" sql-type="varchar(45)" length="45" not-null="true"/>
        </property>
        <set name="restockingsByIdSupplier" inverse="true">
            <key>
                <column name="idSupplier"/>
            </key>
            <one-to-many not-found="ignore" class="fr.iut.model.RestockingEntity"/>
        </set>
        <set name="supplierHasProductsByIdSupplier" inverse="true">
            <key>
                <column name="idSupplier"/>
            </key>
            <one-to-many not-found="ignore" class="fr.iut.model.SupplierHasProductEntity"/>
        </set>
    </class>
</hibernate-mapping>