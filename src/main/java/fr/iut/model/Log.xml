<?xml version="1.0" encoding="UTF-8"?>
<entity-mappings xmlns="http://java.sun.com/xml/ns/persistence/orm"
        version="2.0">

    <entity class="fr.iut.model.Log">
        <table name="Log" schema="CampingSimulator" catalog=""/>
        <attributes>
            <id name="id">
                <column name="id" precision="11"/>
            </id>
            <basic name="datetime">
                <column name="datetime" precision="-1"/>
            </basic>
            <basic name="action">
                <column name="action" length="150"/>
            </basic>
            <basic name="userId">
                <column name="User_id" precision="11"/>
            </basic>
        </attributes>
    </entity>
</entity-mappings>
