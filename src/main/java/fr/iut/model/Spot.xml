<?xml version="1.0" encoding="UTF-8"?>
<entity-mappings xmlns="http://java.sun.com/xml/ns/persistence/orm"
        version="2.0">

    <entity class="fr.iut.model.Spot">
        <table name="Spot" schema="CampingSimulator" catalog=""/>
        <attributes>
            <id name="id">
                <column name="id" precision="11"/>
            </id>
            <basic name="pricePerDay">
                <column name="price_per_day" precision="-1"/>
            </basic>
            <basic name="capacity">
                <column name="capacity" precision="11"/>
            </basic>
            <basic name="water">
                <column name="water" precision="1"/>
            </basic>
            <basic name="electricity">
                <column name="electricity" precision="1"/>
            </basic>
            <basic name="shadow">
                <column name="shadow" precision="1"/>
            </basic>
            <basic name="spotTypeId">
                <column name="SpotType_id" precision="11"/>
            </basic>
        </attributes>
    </entity>
</entity-mappings>
