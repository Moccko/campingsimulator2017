<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="fr.iut.model.AuthorizationEntity" table="Authorization" schema="CampingSimulator">
        <id name="id">
            <column name="id" sql-type="int(11)"/>
        </id>
        <property name="label">
            <column name="label" sql-type="varchar(45)" length="45" not-null="true"/>
        </property>
    </class>
    <class name="fr.iut.model.ClientEntity" table="Client" schema="CampingSimulator">
        <id name="id">
            <column name="id" sql-type="int(11)"/>
        </id>
        <property name="lastName">
            <column name="last_name" sql-type="varchar(45)" length="45"/>
        </property>
        <property name="firstName">
            <column name="first_name" sql-type="varchar(45)" length="45"/>
        </property>
        <property name="phone">
            <column name="phone" sql-type="varchar(11)" length="11" not-null="true"/>
        </property>
        <property name="email">
            <column name="email" sql-type="varchar(45)" length="45" not-null="true"/>
        </property>
    </class>
    <class name="fr.iut.model.ClientHasProblemEntity" table="Client_has_Problem" schema="CampingSimulator">
        <composite-id mapped="true" class="fr.iut.model.ClientHasProblemEntityPK">
            <key-property name="clientId">
                <column name="Client_id" sql-type="int(11)"/>
            </key-property>
            <key-property name="problemId">
                <column name="Problem_id" sql-type="int(11)"/>
            </key-property>
        </composite-id>
    </class>
    <class name="fr.iut.model.EmployeeEntity" table="Employee" schema="CampingSimulator">
        <id name="id">
            <column name="id" sql-type="int(11)"/>
        </id>
        <property name="lastName">
            <column name="last_name" sql-type="varchar(45)" length="45"/>
        </property>
        <property name="firstName">
            <column name="first_name" sql-type="varchar(45)" length="45"/>
        </property>
        <property name="phone">
            <column name="phone" sql-type="varchar(11)" length="11" not-null="true"/>
        </property>
        <property name="email">
            <column name="email" sql-type="varchar(45)" length="45" not-null="true"/>
        </property>
        <property name="completeAddress">
            <column name="complete_address" sql-type="varchar(150)" length="150" not-null="true"/>
        </property>
    </class>
    <class name="fr.iut.model.LocationEntity" table="Location" schema="CampingSimulator">
        <id name="id">
            <column name="id" sql-type="int(11)"/>
        </id>
        <property name="name">
            <column name="name" sql-type="varchar(45)" length="45"/>
        </property>
        <property name="pointX">
            <column name="point_x" sql-type="double" precision="-1" not-null="true"/>
        </property>
        <property name="pointY">
            <column name="point_y" sql-type="double" precision="-1" not-null="true"/>
        </property>
        <one-to-one name="spot" class="fr.iut.model.SpotEntity"/>
    </class>
    <class name="fr.iut.model.LocationHasProblemEntity" table="Location_has_Problem" schema="CampingSimulator">
        <composite-id mapped="true" class="fr.iut.model.LocationHasProblemEntityPK">
            <key-property name="locationId">
                <column name="Location_id" sql-type="int(11)"/>
            </key-property>
            <key-property name="problemId">
                <column name="Problem_id" sql-type="int(11)"/>
            </key-property>
        </composite-id>
    </class>
    <class name="fr.iut.model.LogEntity" table="Log" schema="CampingSimulator">
        <id name="id">
            <column name="id" sql-type="int(11)"/>
        </id>
        <property name="datetime">
            <column name="datetime" sql-type="datetime"/>
        </property>
        <property name="action">
            <column name="action" sql-type="varchar(150)" length="150"/>
        </property>
        <property name="user">
            <column name="user" sql-type="varchar(20)" length="20"/>
        </property>
    </class>
    <class name="fr.iut.model.MapEntity" table="Map" schema="CampingSimulator">
        <property name="image">
            <column name="image" sql-type="mediumblob"/>
        </property>
    </class>
    <class name="fr.iut.model.ProblemEntity" table="Problem" schema="CampingSimulator">
        <id name="id">
            <column name="id" sql-type="int(11)"/>
        </id>
        <property name="appearanceDatetime">
            <column name="appearance_datetime" sql-type="datetime"/>
        </property>
        <property name="label">
            <column name="label" sql-type="varchar(45)" length="45"/>
        </property>
        <property name="solutionDatetime">
            <column name="solution_datetime" sql-type="datetime" not-null="true"/>
        </property>
        <property name="state">
            <column name="state" sql-type="varchar(45)" length="45"/>
        </property>
    </class>
    <class name="fr.iut.model.ProductEntity" table="Product" schema="CampingSimulator">
        <id name="id">
            <column name="id" sql-type="int(11)"/>
        </id>
        <property name="stock">
            <column name="stock" sql-type="int(11)"/>
        </property>
        <property name="sellPrice">
            <column name="sell_price" sql-type="float" precision="-1"/>
        </property>
        <property name="label">
            <column name="label" sql-type="varchar(45)" length="45"/>
        </property>
    </class>
    <class name="fr.iut.model.PurchaseEntity" table="Purchase" schema="CampingSimulator">
        <id name="id">
            <column name="id" sql-type="int(11)"/>
        </id>
        <property name="datetime">
            <column name="datetime" sql-type="datetime"/>
        </property>
        <property name="quantity">
            <column name="quantity" sql-type="int(11)"/>
        </property>
        <property name="productId">
            <column name="Product_id" sql-type="int(11)"/>
        </property>
        <property name="clientId">
            <column name="Client_id" sql-type="int(11)"/>
        </property>
    </class>
    <class name="fr.iut.model.ReservationEntity" table="Reservation" schema="CampingSimulator">
        <id name="id">
            <column name="id" sql-type="int(11)"/>
        </id>
        <property name="clientComment">
            <column name="client_comment" sql-type="varchar(250)" length="250" not-null="true"/>
        </property>
        <property name="starttime">
            <column name="starttime" sql-type="datetime"/>
        </property>
        <property name="endtime">
            <column name="endtime" sql-type="datetime"/>
        </property>
        <property name="reservationDate">
            <column name="reservation_date" sql-type="datetime"/>
        </property>
        <property name="personCount">
            <column name="person_count" sql-type="int(11)"/>
        </property>
        <property name="spotId">
            <column name="Spot_id" sql-type="int(11)"/>
        </property>
        <property name="clientId">
            <column name="Client_id" sql-type="int(11)"/>
        </property>
    </class>
    <class name="fr.iut.model.RestockingEntity" table="Restocking" schema="CampingSimulator">
        <id name="id">
            <column name="id" sql-type="int(11)"/>
        </id>
        <property name="quantity">
            <column name="quantity" sql-type="int(11)"/>
        </property>
        <property name="datetime">
            <column name="datetime" sql-type="datetime"/>
        </property>
        <property name="supplierId">
            <column name="Supplier_id" sql-type="int(11)"/>
        </property>
        <property name="productId">
            <column name="Product_id" sql-type="int(11)"/>
        </property>
    </class>
    <class name="fr.iut.model.SpotEntity" table="Spot" schema="CampingSimulator">
        <id name="id">
            <column name="id" sql-type="int(11)"/>
        </id>
        <property name="pricePerDay">
            <column name="price_per_day" sql-type="float" precision="-1"/>
        </property>
        <property name="capacity">
            <column name="capacity" sql-type="int(11)"/>
        </property>
        <property name="water">
            <column name="water" sql-type="tinyint(1)"/>
        </property>
        <property name="electricity">
            <column name="electricity" sql-type="tinyint(1)"/>
        </property>
        <property name="shadow">
            <column name="shadow" sql-type="tinyint(1)"/>
        </property>
        <property name="spotTypeId">
            <column name="SpotType_id" sql-type="int(11)"/>
        </property>
        <one-to-one name="location" class="fr.iut.model.LocationEntity"/>
    </class>
    <class name="fr.iut.model.SpotTypeEntity" table="SpotType" schema="CampingSimulator">
        <id name="id">
            <column name="id" sql-type="int(11)"/>
        </id>
        <property name="label">
            <column name="label" sql-type="varchar(45)" length="45"/>
        </property>
    </class>
    <class name="fr.iut.model.SupplierEntity" table="Supplier" schema="CampingSimulator">
        <id name="id">
            <column name="id" sql-type="int(11)"/>
        </id>
        <property name="name">
            <column name="name" sql-type="varchar(45)" length="45"/>
        </property>
        <property name="phone">
            <column name="phone" sql-type="varchar(11)" length="11" not-null="true"/>
        </property>
        <property name="email">
            <column name="email" sql-type="varchar(45)" length="45" not-null="true"/>
        </property>
        <property name="website">
            <column name="website" sql-type="varchar(45)" length="45" not-null="true"/>
        </property>
    </class>
    <class name="fr.iut.model.SupplierHasProductEntity" table="Supplier_has_Product" schema="CampingSimulator">
        <composite-id mapped="true" class="fr.iut.model.SupplierHasProductEntityPK">
            <key-property name="supplierId">
                <column name="Supplier_id" sql-type="int(11)"/>
            </key-property>
            <key-property name="productId">
                <column name="Product_id" sql-type="int(11)"/>
            </key-property>
        </composite-id>
        <property name="buyPrice">
            <column name="buy_price" sql-type="float" precision="-1"/>
        </property>
    </class>
    <class name="fr.iut.model.TaskEntity" table="Task" schema="CampingSimulator">
        <id name="idTask">
            <column name="idTask" sql-type="int(11)"/>
        </id>
        <property name="starttime">
            <column name="starttime" sql-type="datetime"/>
        </property>
        <property name="endtime">
            <column name="endtime" sql-type="datetime"/>
        </property>
        <property name="label">
            <column name="label" sql-type="varchar(50)" length="50"/>
        </property>
        <property name="employeeId">
            <column name="Employee_id" sql-type="int(11)"/>
        </property>
        <property name="locationId">
            <column name="Location_id" sql-type="int(11)" not-null="true"/>
        </property>
    </class>
    <class name="fr.iut.model.UserEntity" table="User" schema="CampingSimulator">
        <id name="id">
            <column name="id" sql-type="int(11)"/>
        </id>
        <property name="login">
            <column name="login" sql-type="varchar(20)" length="20"/>
        </property>
        <property name="password">
            <column name="password" sql-type="mediumtext"/>
        </property>
        <property name="employeeId">
            <column name="Employee_id" sql-type="int(11)" not-null="true"/>
        </property>
    </class>
    <class name="fr.iut.model.UserHasAuthorizationEntity" table="User_has_Authorization" schema="CampingSimulator">
        <composite-id mapped="true" class="fr.iut.model.UserHasAuthorizationEntityPK">
            <key-property name="authorizationId">
                <column name="Authorization_id" sql-type="int(11)"/>
            </key-property>
            <key-property name="userId">
                <column name="User_id" sql-type="int(11)"/>
            </key-property>
        </composite-id>
    </class>
    <class name="fr.iut.model.PurchasesOnReservationEntity" table="PurchasesOnReservation" schema="CampingSimulator">
        <property name="reservationId">
            <column name="Reservation_id" sql-type="int(11)"/>
        </property>
        <property name="purchaseId">
            <column name="Purchase_id" sql-type="int(11)"/>
        </property>
    </class>
</hibernate-mapping>